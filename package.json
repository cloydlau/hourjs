{
  "name": "hourjs",
  "version": "0.1.1",
  "description": "Sometimes we deal with TIME ONLY strings like '10:01' or '10:01:02' which are considered wrong formats by dayjs or momentjs.",
  "author": "cloydlau",
  "private": false,
  "license": "MIT",
  "main": "dist/hourjs.umd.min.js",
  "module": "dist/hourjs.esm.min.js",
  "browser": "dist/hourjs.iife.min.js",
  "scripts": {
    "build": "rollup -c",
    "cov": "jest --ci --coverage",
    "dev": "rollup -c -w --environment NODE_ENV:development",
    "jest": "jest --watchAll",
    "pretest": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/preset-env": "^7.12.0",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.1.0",
    "codecov": "^3.8.0",
    "jest": "^26.6.0",
    "rollup": "^2.35.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.0",
    "ts-jest": "^26.4.0",
    "tslib": "^2.0.0",
    "typescript": "^4.1.0"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
