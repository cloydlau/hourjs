{
  "name": "hourjs",
  "version": "0.1.1",
  "description": "Sometimes we deal with TIME ONLY strings like '10:01' or '10:01:02' which are considered wrong formats by dayjs or momentjs.",
  "author": "cloydlau",
  "private": false,
  "license": "MIT",
  "main": "dist/hourjs.umd.min.js",
  "module": "dist/hourjs.esm.min.js",
  "scripts": {
    "build": "rollup -c",
    "cov": "jest --ci --coverage",
    "dev": "rollup -c -w --environment NODE_ENV:development",
    "jest": "jest --watchAll",
    "pretest": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7",
    "@babel/preset-env": "7",
    "@rollup/plugin-babel": "5",
    "@rollup/plugin-commonjs": "17",
    "@rollup/plugin-json": "4",
    "@rollup/plugin-node-resolve": "11",
    "@rollup/plugin-typescript": "8",
    "codecov": "3",
    "jest": "26",
    "rollup": "2",
    "rollup-plugin-livereload": "2",
    "rollup-plugin-serve": "1",
    "rollup-plugin-terser": "7",
    "ts-jest": "26",
    "tslib": "2",
    "typescript": "4"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
